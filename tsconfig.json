{
    "compilerOptions": {
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
  
      "target": "es5",
      "module": "commonjs",
      "jsx": "react",
      "jsxFactory": "h",
      "sourceMap": true,
      "baseUrl": "./",
      "paths": {
        "~/*": ["src/*"]
      },
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "lib": ["dom", "webworker"]
    }
  }
  